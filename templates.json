[
	

	





	{
		"type": 1,
		"title": "Jackett",
		"name": "jackett",
		"description": "Jackett funciona como um servidor proxy. Um agregador de indexadores. Ele Ã© essencial para que o Sonarr, Radarr, Lidarr e Transmission funcionem adequadamente.",
		"logo": "https://raw.githubusercontent.com/alexbarlim/starks-portainer/master/imgs/jackett.png",
		"image": "linuxserver/jackett:latest",
		"categories": [
		  "All"
		],
		"platform": "linux",
		"restart_policy": "unless-stopped",
		"network": "bridge",
		"ports": [
		  "9117:9117/tcp"
		],
		"volumes": [
		  {
			"container": "/config",
			"bind": "/srv/dev-disk-by-label-6TB/appdata/jackett/data"
		  },
		  {
			"container": "/downloads",
			"bind": "/srv/dev-disk-by-label-6TB/appdata/jackett/blackhole"
		  }
		],
		"env": [
		  {
			"name": "PUID",
			"label": "PUID",
			"default": "1000"
		  },
		  {
			"name": "PGID",
			"label": "PGID",
			"default": "100"
		  },
		  {
			"name": "TZ",
			"label": "TZ",
			"default": "America/Sao_Paulo"
		  },
		  {
			"name": "UMASK_SET",
			"label": "UMASK_SET",
			"default": "022"
		  }
		]
	  }









]
